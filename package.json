{
  "name": "compass",
  "main": "src/index.ts",
  "engines": {
    "node": "20 || 22"
  },
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "start:prod": "next start",
    "build": "next build && cp -r ./prompts ./.next/prompts",
    "build:clean": "rm -rf ./next",
    "dev": "genkit start -- npx nodemon --ext prompt --watch prompts --exec next dev",
    "dev:next": "next dev",
    "dev:genkit": "genkit start -- npx nodemon --ext prompt --watch prompts --exec \"tsx --watch src/index.ts\"",
    "lint": "next lint",
    "verify": "node scripts/verify-setup.js"
  },
  "dependencies": {
    "@genkit-ai/firebase": "^0.9",
    "@genkit-ai/vertexai": "^0.9",
    "clsx": "^2.1.1",
    "firebase-admin": "^12.2.0",
    "genkit": "^0.9",
    "next": "14.2.35",
    "react": "^18",
    "react-dom": "^18",
    "sass": "1.91.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "^15.5.9",
    "genkit-cli": "^0.9.12",
    "nodemon": "^3.0.0",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.6",
    "tsx": "^4.0.0",
    "typescript": "^5.5.3"
  }
}
